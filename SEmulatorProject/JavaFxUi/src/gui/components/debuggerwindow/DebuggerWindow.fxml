<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.debuggerwindow.DebuggerWindowController">
   <children>
      <fx:include fx:id="debuggerCommandsBar" source="../debuggercommandsbar/DebuggerCommandsBar.fxml" />
      <HBox prefHeight="243.0" prefWidth="600.0" spacing="10.0">
         <children>
            <VBox prefHeight="243.0" prefWidth="368.0">
               <children>
                  <TableView fx:id="variablesDisplayTable" prefHeight="194.0" prefWidth="373.0">
                    <columns>
                      <TableColumn fx:id="colVariable" prefWidth="126.0" text=" Variable" />
                      <TableColumn fx:id="colVariableValue" prefWidth="171.0" text="Value" />
                    </columns>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TableView>
                  <Label prefHeight="25.0" prefWidth="371.0" style="-fx-border-color: black; -fx-border-width: 0.5; -fx-border-radius: 5;" text="Cycles: ">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="243.0" prefWidth="211.0" style="-fx-border-color: gray;">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="23.0" prefWidth="222.0" style="-fx-background-color: gray;" text="Input Variables Picker" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="7.0" />
                     </VBox.margin>
                  </Label>
                  <VBox fx:id="inputVariablesContainer" alignment="TOP_CENTER" prefHeight="220.0" prefWidth="209.0" />
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
