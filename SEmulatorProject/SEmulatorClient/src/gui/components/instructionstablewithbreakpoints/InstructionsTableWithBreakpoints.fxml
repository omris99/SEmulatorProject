<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="390.0" prefWidth="600.0" stylesheets="@/gui/components/instructionstable/InstructionsTable.css" xmlns="http://javafx.com/javafx/21.0.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.instructionstablewithbreakpoints.InstructionsTableWithBreakpointsController">
    <children>
        <TableView fx:id="InstructionsTable" prefHeight="413.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="colBreakpoint" prefWidth="90.0" text="Breakpoint" />
