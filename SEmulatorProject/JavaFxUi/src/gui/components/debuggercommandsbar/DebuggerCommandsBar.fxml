<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" prefHeight="57.0" prefWidth="685.0" prefWrapLength="100.0" stylesheets="@DebuggerCommandsBar.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.debuggercommandsbar.debuggerCommandsBarController">
   <children>
      <Button fx:id="newRunButton" disable="true" mnemonicParsing="false" onAction="#newRunButtonAction" prefHeight="42.0" prefWidth="75.0" text="New Run">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button id="startButton" fx:id="startButton" disable="true" mnemonicParsing="false" onAction="#startButtonAction" prefHeight="42.0" prefWidth="75.0" text="START" textAlignment="CENTER" wrapText="true">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button fx:id="debugButton" disable="true" mnemonicParsing="false" onAction="#debugButtonAction" prefHeight="42.0" prefWidth="75.0" text="DEBUG">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button fx:id="resumeButton" disable="true" mnemonicParsing="false" onAction="#resumeButtonAction" prefHeight="42.0" prefWidth="75.0" text="RESUME">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button fx:id="stepOverButton" disable="true" mnemonicParsing="false" onAction="#stepOverButtonAction" prefHeight="42.0" prefWidth="75.0" text="Step Over">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button fx:id="stepBackwardButton" disable="true" mnemonicParsing="false" onAction="#stepBackwardButtonAction" prefHeight="42.0" prefWidth="75.0" text="Step Backward" textAlignment="CENTER" wrapText="true">
         <FlowPane.margin>
            <Insets right="10.0" top="10.0" />
         </FlowPane.margin>
      </Button>
      <Button fx:id="stopButton" disable="true" mnemonicParsing="false" onAction="#stopButtonAction" prefHeight="42.0" prefWidth="75.0" text="STOP !" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS" wrapText="true">
         <FlowPane.margin>
            <Insets top="10.0" />
         </FlowPane.margin></Button>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</FlowPane>
