<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefWidth="600.0" style="-fx-border-color: blue;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.debuggerwindow.DebuggerWindowController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="745.0" style="-fx-background-color: blue;" text="Execution Window" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <fx:include fx:id="debuggerCommandsBar" source="../debuggercommandsbar/DebuggerCommandsBar.fxml" />
      <HBox alignment="CENTER" prefHeight="243.0" prefWidth="600.0" spacing="10.0">
         <children>
            <fx:include fx:id="executionStateWindow" source="../executionstatewindow/ExecutionStateWindow.fxml" />
            <VBox alignment="TOP_CENTER" prefHeight="243.0" prefWidth="211.0" style="-fx-border-color: gray;">
               <children>
                  <Label alignment="TOP_CENTER" prefHeight="23.0" prefWidth="222.0" style="-fx-background-color: gray;" text="Input Variables Picker" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="7.0" />
                     </VBox.margin>
                  </Label>
                  <VBox fx:id="inputVariablesContainer" alignment="TOP_CENTER" prefHeight="220.0" prefWidth="209.0" />
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
