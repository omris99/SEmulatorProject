<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="440.0" prefWidth="833.0" xmlns="http://javafx.com/javafx/21.0.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.availablefunctionstable.AvailableFunctionsTableController">
   <children>
      <TableView fx:id="functionsTable" prefHeight="538.0" prefWidth="833.0">
        <columns>
          <TableColumn fx:id="colName" prefWidth="103.74083864688873" text="Name" />
            <TableColumn fx:id="colContextProgram" prefWidth="151.0" text="Context Program" />
            <TableColumn fx:id="colUploadedBy" prefWidth="158.44225311279297" text="Uploaded By" />
            <TableColumn fx:id="colInstructionsCount" prefWidth="165.24786376953125" text="Instructions Count" />
            <TableColumn fx:id="colMaximalDegree" prefWidth="130.48178100585938" text="Maximal Degree" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="executeFunctionButton" alignment="CENTER" mnemonicParsing="false" onAction="#onExecuteFunctionButtonClick" prefHeight="33.0" prefWidth="143.0" text="Execute Function">
         <VBox.margin>
            <Insets right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
