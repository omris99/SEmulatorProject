<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="440.0" prefWidth="833.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.availableusers.AvailableUsersController">
   <children>
      <TableView fx:id="usersTable" prefHeight="538.0" prefWidth="833.0">
        <columns>
          <TableColumn fx:id="colUserName" prefWidth="103.74083864688873" text="User Name" />
            <TableColumn fx:id="colMainProgramsUploaded" prefWidth="158.44225311279297" text="Main Programs Uploaded" />
            <TableColumn fx:id="colFunctionsContributed" prefWidth="165.24786376953125" text="Functions Contributed" />
            <TableColumn fx:id="colCreditBalance" prefWidth="130.48178100585938" text="Credit Balance" />
            <TableColumn fx:id="colCreditsUsed" prefWidth="99.363525390625" text="Credits Used" />
            <TableColumn fx:id="colExecutionsPerformed" prefWidth="170.63653564453125" text="Executions Performed" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="unselectUserButton" alignment="CENTER" mnemonicParsing="false" onAction="#onUnselectButtonClick" prefHeight="33.0" prefWidth="143.0" text="Unselect User">
         <VBox.margin>
            <Insets right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
