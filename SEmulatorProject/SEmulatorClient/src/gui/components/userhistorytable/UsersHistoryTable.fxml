<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="297.0" prefWidth="833.0" xmlns="http://javafx.com/javafx/21.0.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.userhistorytable.UsersHistoryTableController">
   <children>
      <TableView fx:id="userHistoryTable" editable="true" prefHeight="245.0" prefWidth="833.0">
        <columns>
          <TableColumn fx:id="colExecutionNumber" prefWidth="103.74083864688873" text="# Execution" />
            <TableColumn fx:id="colProgramType" prefWidth="139.0" text="Program / Function" />
            <TableColumn fx:id="colProgramName" prefWidth="140.0" text="Name" />
            <TableColumn fx:id="colArchitecture" prefWidth="105.0" text="Architecture" />
            <TableColumn fx:id="colDegree" prefWidth="72.0" text="Degree" />
            <TableColumn fx:id="colYResult" prefWidth="95.0" text="Y-Result" />
            <TableColumn fx:id="colTotalCycles" prefWidth="208.0" text="Total Cycles" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <GridPane hgap="10.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="24.0" minHeight="10.0" prefHeight="12.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="62.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button fx:id="showButton" mnemonicParsing="false" onAction="#onShowButtonClick" prefHeight="33.0" prefWidth="143.0" text="Show" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="reRunButton" mnemonicParsing="false" onAction="#onReRunButtonClick" prefHeight="56.0" prefWidth="142.0" text="Re-Run" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</VBox>
