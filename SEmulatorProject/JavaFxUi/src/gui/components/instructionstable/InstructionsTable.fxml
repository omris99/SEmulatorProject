<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="390.0" prefWidth="600.0" stylesheets="@InstructionsTable.css" xmlns="http://javafx.com/javafx/21.0.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.instructionstable.InstructionsTableController">
   <children>
      <TableView fx:id="InstructionsTable" prefHeight="413.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="ColIndex" prefWidth="40.0" text="#" />
          <TableColumn fx:id="colInstructionType" prefWidth="139.0" text="Basic / Synthetic" />
            <TableColumn fx:id="colLabel" prefWidth="53.0" text="Label" />
            <TableColumn fx:id="colInstructionDisplayFormat" prefWidth="277.0" text="Instruction" />
            <TableColumn fx:id="colCycle" prefWidth="90.0" text="Cycles" />
        </columns>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
