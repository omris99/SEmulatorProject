<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.components.debuggerwindow.DebuggerWindowController">
   <children>
      <fx:include fx:id="debuggerCommandsBar" source="../debuggercommandsbar/DebuggerCommandsBar.fxml" />
      <HBox prefHeight="243.0" prefWidth="600.0" spacing="10.0">
         <children>
            <VBox prefHeight="239.0" prefWidth="298.0">
               <children>
                  <TableView fx:id="variablesDisplayTable" prefHeight="194.0" prefWidth="257.0">
                    <columns>
                      <TableColumn fx:id="colVariable" prefWidth="126.0" text=" Variable" />
                      <TableColumn fx:id="colVariableValue" prefWidth="171.0" text="Value" />
                    </columns>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TableView>
                  <Label prefHeight="25.0" prefWidth="371.0" style="-fx-border-color: black; -fx-border-width: 0.5; -fx-border-radius: 5;" text="Cycles: ">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
            <TableView fx:id="inputVariablesTable" prefHeight="230.0" prefWidth="293.0">
              <columns>
                <TableColumn fx:id="colInputVariable" editable="false" prefWidth="130.0" resizable="false" sortable="false" text="Input Variable" />
                <TableColumn fx:id="colInputVariableValue" prefWidth="162.0" resizable="false" sortable="false" text="Value" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
